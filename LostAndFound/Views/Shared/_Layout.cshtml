    <script type="text/javascript">

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
            location.reload(true);
        }

        function getCookie(name) {
            var re = new RegExp(name + "=([^;]+)");
            var value = re.exec(document.cookie);
            return (value != null) ? unescape(value[1]) : null;
        }

        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $('#lanBAN').click(function () { setCookie('lang', 'bn', 30); });
            $('#lanENG').click(function () { setCookie('lang', 'en', 30); });

            @*$('#menu').load('@Url.Action("Navigation", "Home")');*@
            //$('#menu').load('@Url.Action("AssignPage", "Home")');
            var lang = getCookie('lang');

            $("#chatbot-chat").hide();
            $("#btnclose").hide();
            var BaseUrl = "";
            var ip = location.host;
            if (ip == 'localhost:23999') {
                BaseUrl = 'http://localhost:23999/CustoomChatBox?lang=' + lang;
            }
            else {
                BaseUrl = 'http://103.134.88.13:122/CustoomChatBox?lang=' + lang;
            }
            var i = 0;
            $("#test").click(function () {
                if (i % 2 == 0) {
                    $('#chatbot-chat-frame').prop('src', BaseUrl);
                    $("#chatbot-chat").show();
                    $("#btnclose").show();
                } else {
                    $('#chatbot-chat-frame').removeAttr('src');
                    $("#chatbot-chat").hide();
                    $("#btnclose").hide();
                }
                i++;
            });

            $("#btnclose").click(function () {
                window.location.reload();
                $("#btnclose").hide();
            });


            // Password Validation

            $('.customSelect').select2();
            $(".errorMessage").hide();

            $("#Password").keyup(function () {

                var pass = $("#Password").val();
                var confirmPass = $("#ConfirmPassword").val();

                if (pass != '' && pass != null && confirmPass != '' && confirmPass != null) {

                    if (pass != confirmPass) {

                        $(".errorMessage").text("password and confirm password does't match");
                        $(".errorMessage").show();
                    }
                    else {

                        $(".errorMessage").hide();
                    }
                }

            });

            $("#ConfirmPassword").keyup(function () {

                var pass = $("#Password").val();
                var confirmPass = $("#ConfirmPassword").val();


                if (pass != '' && pass != null && confirmPass != '' && confirmPass != null) {

                    if (pass != confirmPass) {

                        $(".errorMessage").text("password and confirm password does't match");
                        $(".errorMessage").show();
                    }
                    else {

                        $(".errorMessage").hide();
                    }
                }

            });

            // end password validation

        });

        function changePassBtnSubmit() {

            var myData = $("#changePasswordForm").serialize();


            var oldPass = $("#OldPassword").val();
            var pass = $("#Password").val();
            var confirmPass = $("#ConfirmPassword").val();


            if (oldPass == null || oldPass == '') {

                swal('Warning', 'Invalid OldPassword!', 'warning');
                return false;
            }
            else if (pass == null || pass == '') {

                swal('Warning', 'Invalid Password!', 'warning');
                return false;
            }
            else if (confirmPass == null || confirmPass == '') {

                swal('Warning', 'Invalid Confirm Password!', 'warning');
                return false;
            }
            else if ((pass != '' && pass != null && confirmPass != '' && confirmPass != null)) {

                if (pass != confirmPass) {

                    $(".errorMessage").text("password and confirm password does't match");
                    $(".errorMessage").show();

                    swal('Warning', 'Password and Confirm Password Does Not Match!', 'warning');
                    return false;
                }
            }



            $.ajax({

                url: "/Auth/Account/ChangePassword",
                type: "post",
                data: myData,
                success: function (response) {

                    if (response == "success") {

                        swal({
                            title: 'Success!', text: 'Password Update Successfully', type: 'success', confirmButtonColor: '#3085d6', confirmButtonText: 'Ok!',
                        }).then(function () {
                            window.location.href = "/Home/Index";
                        });
                    }
                    else {

                        swal('error', 'Something is Wrong Please Try Again !!!', 'error');
                        return false;
                    }

                }
            });

        }

        function ClearLocalStorage() {
            window.localStorage.clear();
        }

        var BaseUrl = location.host;

    </script>
